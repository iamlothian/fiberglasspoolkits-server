    "start": "node ./dist/index.js",
    "debug": "node --inspect=49559 --debug-brk ./dist/index.js",
    "build": "tsc -w",
    "docker:mongo": "docker run -d -p 27017:27017 --name mongo mongo --auth && powershell sleep 1 && powershell ./db/init.cmd && docker restart mongo",
